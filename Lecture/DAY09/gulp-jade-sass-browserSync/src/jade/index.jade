-var nav_contents = ['home', 'about', 'works', 'contact'];

<!DOCTYPE html>
html(lang="ko-KR")
head
    meta(http-equiv="X-UA-Compatible", content="IE=Edge")
    meta(charset="UTF-8")
    title Making jQuery UI Component
    meta(name="viewport", content="width=device-width, initial-scale=1")
    link(rel="stylesheet", href="css/style.css")
    script(src="js/lib/jquery.min.js")
    script(src="js/app.js")

body
    //- skip navigation
    .skip
        each val in [1, 2, 3]
            a(href="#point#{val}") point #{val}

    header#page-header
        h1.brand Page Title
        p.slogan
            | Lorem ipsum dolor sit amet,
            | consectetur adipisicing elit.

    nav
        ul(role="presentation")
            each val in nav_contents
                li(role="menu-item")
                    a(href="#") #{val}

